o/react-portfolio/src/App.js",["77"],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/AboutSection.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/reportWebVitals.js",[],["78","79"],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/pages/AboutUs.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/ServicesSection.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/FaqSection.js",["80"],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/GlobalStyled.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/styles.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/Nav.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/pages/OurWork.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/pages/Contact.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/pages/MovieDetail.js",["81"],"import React, { useState, useEffect } from \"react\";\nimport styled from \"styled-components\";\nimport { useHistory } from \"react-router-dom\";\nimport { MovieState } from \"../movieState\";\n\n// ANIMATIONS\nimport { motion } from \"framer-motion\";\nimport { pageAnimation } from \"../animation\";\n\nimport ScrollTop from \"../components/scrollTop\";\n\nconst MovieDetail = () => {\n  const history = useHistory();\n  const url = history.location.pathname;\n  const [movies, setMovies] = useState(MovieState);\n  // We use this extract the movie based on the url\n  const [movie, setMovie] = useState(null);\n\n  // USEEFFECT\n  /* As soon as the component mounts. When we click on a movie, it's to load that movies specific information. So the useEffect is going to run as soon as the component mounts. */\n\n  useEffect(() => {\n    //1. Here we find out which movie we are on, we compare the the captured url against the array of movies and filter a match\n    //2. Then we update our state.\n    //3. Lastly, we tell the useEffect to run everytime movies or url updates\n    const currentMovie = movies.filter((stateMovie) => stateMovie.url === url);\n    setMovie(currentMovie[0]);\n  }, [movies, url]);\n\n  return (\n    /* Because our useEffect runs immediately, we don't want to render the page until we have the data we need to render the components correctly. In this case we need to  identify whch data to render based on the filter function we use above.\n\n    So by Wrapping our components in curly brackets and adding some logic, we can tell te app to render only if 'movie' is true and not null as we set for the default state. > {movie && (...)}\n    */\n    <>\n      {movie && (\n        <Details\n          exit=\"exit\"\n          variants={pageAnimation}\n          initial=\"hidden\"\n          animate=\"show\"\n        >\n          <HeadLine>\n            <h2>{movie.title}</h2>\n            <img src={movie.mainImg} alt=\"movie\" />\n          </HeadLine>\n          <Awards>\n            {movie.awards.map((award) => (\n              <Award\n                title={award.title}\n                description={award.description}\n                key={award.title}\n              />\n            ))}\n          </Awards>\n          <ImageDisplay>\n            <img src={movie.secondaryImg} alt=\"Movie\" />\n          </ImageDisplay>\n          <ScrollTop />\n        </Details>\n      )}\n    </>\n  );\n};\n\nconst Details = styled(motion.div)``;\n\nconst HeadLine = styled.div`\n  min-height: 90vh;\n  padding-top: 20vh;\n  position: relative;\n  h2 {\n    position: absolute;\n    top: 10%;\n    left: 50%;\n    transform: translate(-50%, -10%);\n  }\n  img {\n    width: 100%;\n    height: 70vh;\n    object-fit: cover;\n  }\n`;\n\nconst Awards = styled.div`\n  min-height: 80vh;\n  display: flex;\n  margin: 5rem 10rem;\n  align-items: center;\n  justify-content: space-around;\n  @media (max-width: 1300px) {\n    flex-direction: column;\n    justify-content: center;\n    margin: 2rem;\n  }\n`;\n\nconst StyledAward = styled.div`\n  padding: 5rem;\n\n  h3 {\n    font-size: 2rem;\n  }\n  .line {\n    width: 50%;\n    height: 0.5rem;\n    margin: 1rem 0;\n    background-color: #23d997;\n  }\n  p {\n    padding: 2rem 0;\n  }\n`;\n\nconst ImageDisplay = styled.div`\n  min-height: 50vh;\n  img {\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n`;\n\n// AWARD COMPONENT\nconst Award = ({ title, description }) => {\n  return (\n    <StyledAward>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </StyledAward>\n  );\n};\n\nexport default MovieDetail;\n","/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/animation.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/Wave.js",["82"],"import react from \"react\";\nimport styled from \"styled-components\";\nimport { motion } from \"framer-motion\";\n\nconst Wave = () => {\n  return (\n    <WaveSvg\n      viewBox=\"0 0 1440 363\"\n      fill=\"none\"\n      xmlns=\"http://www.w3.org/2000/svg\"\n    >\n      <motion.path\n        initial={{ pathLength: 0, pathOffset: 1 }}\n        animate={{ pathLength: 1, pathOffset: 0 }}\n        transition={{ duration: 2 }}\n        d=\"M1440 27.4774C1352.73 19.8184 1122.41 49.0556 899.331 227.276C620.48 450.052 354.282 355.647 170.328 185.318C23.165 49.0556 -4.21721 8.32998 0.487081 5\"\n        stroke=\"#D96ED4\"\n        strokeOpacity=\"0.1\"\n        strokeWidth=\"10\"\n      />\n    </WaveSvg>\n  );\n};\n\nconst WaveSvg = styled.svg`\n  position: absolute;\n  left: 0;\n  z-index: -1;\n`;\n\nexport default Wave;\n","/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/toggle.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/useScroll.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Lesson_and_Exercises/React/react-portfolio/react-portfolio/src/components/scrollTop.js",[],{"ruleId":"83","severity":1,"message":"84","line":15,"column":8,"nodeType":"85","messageId":"86","endLine":15,"endColumn":17},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"83","severity":1,"message":"91","line":1,"column":17,"nodeType":"85","messageId":"86","endLine":1,"endColumn":25},{"ruleId":"83","severity":1,"message":"92","line":15,"column":18,"nodeType":"85","messageId":"86","endLine":15,"endColumn":27},{"ruleId":"83","severity":1,"message":"93","line":1,"column":8,"nodeType":"85","messageId":"86","endLine":1,"endColumn":13},"no-unused-vars","'ScrollTop' is defined but never used.","Identifier","unusedVar","no-native-reassign",["94"],"no-negated-in-lhs",["95"],"'useState' is defined but never used.","'setMovies' is assigned a value but never used.","'react' is defined but never used.","no-global-assign","no-unsafe-negation"]